 <learningAssessmentbody>
  <lcInteraction>
   <lcSequencing id="sequencing_elements">
    <lcQuestion>Mettez les éléments suivants dans leur ordre d'apparition dans une tâche. </lcQuestion>
    <lcSequenceOptionGroup id="lcSequenceOptionGroup_qqp_qzb_kt">
     <lcSequenceOption>
      <lcAnswerContent>&lt;example></lcAnswerContent>
      <lcSequence value="5"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;steps></lcAnswerContent>
      <lcSequence value="3"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;postreq></lcAnswerContent>
      <lcSequence value="6"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;result></lcAnswerContent>
      <lcSequence value="4"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;prereq></lcAnswerContent>
      <lcSequence value="1"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;context></lcAnswerContent>
      <lcSequence value="2"/>
     </lcSequenceOption>
    </lcSequenceOptionGroup>
    <lcFeedbackIncorrect>Ces éléments doivent apparaître dans l'ordre suivant&#xA0;: &lt;prereq>,
     &lt;context>, &lt;steps>, &lt;result>, &lt;example>, &lt;postreq>.</lcFeedbackIncorrect>
   </lcSequencing>
  </lcInteraction>
