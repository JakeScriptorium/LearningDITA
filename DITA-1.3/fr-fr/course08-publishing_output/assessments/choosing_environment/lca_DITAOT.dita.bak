     <lcAnswerOption>
      <lcAnswerContent>Sortie personnalisée</lcAnswerContent>
     </lcAnswerOption>
     <lcAnswerOption>
      <lcAnswerContent>InDesign</lcAnswerContent>
     </lcAnswerOption>
     <lcAnswerOption>
      <lcAnswerContent>PDF</lcAnswerContent>
      <lcCorrectResponse/>
     </lcAnswerOption>
     <lcAnswerOption>
      <lcAnswerContent>HTML</lcAnswerContent>
      <lcCorrectResponse/>
     </lcAnswerOption>
    </lcAnswerOptionGroup>
    <lcFeedbackIncorrect>Par défaut, DITA OT inclut des extensions pour publier dans les formats PDF
     et HTML.</lcFeedbackIncorrect>
   </lcMultipleSelect>
